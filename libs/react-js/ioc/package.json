{
  "name": "@norabytes/reactjs-ioc",
  "version": "1.1.4",
  "author": "Adi-Marian Mutu <adi.mutu@norabytes.com>",
  "description": "A simple Inversion of Control library for ReactJS built on top of the injection-js library.",
  "homepage": "https://gitlab.com/norabytes/typescript/-/blob/main/libs/react-js/ioc/README.md",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/norabytes/typescript/-/blob/main/libs/react-js/ioc"
  },
  "license": "MIT",
  "keywords": [
    "inversion of control",
    "ioc",
    "react",
    "reactjs",
    "di",
    "dependency injection"
  ],
  "private": false,
  "scripts": {
    "lint": "nx lint reactjs-ioc",
    "build": "npm run lint && nx build reactjs-ioc",
    "publish-lib": "npm run build && npm --prefix libs/react-js/ioc/dist publish --access public"
  },
  "dependencies": {
    "injection-js": "2.4.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "type-fest": "^4.21.0",
    "typescript": "^4.7.0"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "type": "commonjs",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/src/index.js",
      "import": "./dist/src/index.js",
      "types": "./dist/src/types/injector/index.d.ts"
    },
    "./r": {
      "require": "./dist/src/react/index.js",
      "import": "./dist/src/react/index.js",
      "types": "./dist/src/types/react/index.d.ts"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.map",
    "!dist/src/types/*.js"
  ]
}
