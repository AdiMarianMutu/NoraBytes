{
  "name": "reactjs-ioc",
  "author": "Adi-Marian Mutu",
  "description": "A simple Inversion of Control library for `ReactJS` built on top of the `injection-js` library.",
  "license": "MIT",
  "keywords": ["inversion of control", "ioc", "react", "reactjs", "di", "dependency injection"],
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/react-js/ioc/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/react-js/ioc/dist",
        "main": "libs/react-js/ioc/src/index.ts",
        "tsConfig": "libs/react-js/ioc/tsconfig.lib.json",
        "assets": ["libs/react-js/ioc/*.md"]
      }
    }
  }
}
